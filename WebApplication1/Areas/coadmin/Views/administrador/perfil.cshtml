                            <p>Escritura de compra-venta.</p>
                            <div class="form-group row">
                                <label for="example-name2-input" class="col-sm-3 col-form-label">Subir Escritura</label>
                                <div class="col-sm-9">
                                    
                                    <div class="input-group mt-2">
                                        <div class="custom-file" ">


                                           

                                            <input type="file" class="custom-file-input" id="inputGroupFile04"
                                                   name="upload_writing" data-toggle="tooltip" title="Dirección residencial del Titular" />
                                            <label class="custom-file-label" for="inputGroupFile04">Seleccionar archivo</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-name2-input" class="col-sm-3 col-form-label">Descargar archivo</label>
                                <div class="col-sm-9">
                                    <div class="input-group mt-2">
                                        <div class="custom-file">
                                            @if (Model.curUser.upload_writing != null)
                                            {
                                                <a href="~/Upload/Upload_Writing/@Model.curUser.upload_writing">
                                                    <button type="button" class="btn btn-info waves-effect waves-light">
                                                        <i class="mdi mdi-cloud-download  m-r-5"></i> Descargar archivo
                                                    </button>
                                                </a>
                                            }
                                            else
                                            {
                                                <button type="button" class="btn btn-info waves-effect waves-light">
                                                    <i class="mdi mdi-cloud-download  m-r-5"></i> Descargar archivo
                                                </button>
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-divider"></div>
                            <div class="form-group row">
                                <label for="example-address1-input" class="col-sm-3 col-form-label">¿Está la propiedad arrendada?</label>
                                <div class="col-sm-9">
                                    <div class="form-check-inline my-2">
                                        <div class="custom-control custom-checkbox">
                                            @if (Model.curUser.siono == "boxyes")
                                            {
                                                <input type="checkbox" name="siono" value="boxyes" id="id-box-yes"
                                                       class="custom-control-input"
                                                       checked onclick="onBoxYes()">
                                            }
                                            else
                                            {
                                                <input type="checkbox" name="siono" value="boxyes" id="id-box-yes"
                                                       class="custom-control-input"
                                                       onclick="onBoxYes()">
                                            }
                                            <label class="custom-control-label" for="id-box-yes">
                                                Si
                                            </label>
                                        </div>
    
                                        </div>
    
                                        <div class="form-check-inline my-2">
                                            <div class="custom-control custom-checkbox">
                                                @if (Model.curUser.siono == "boxno" || Model.curUser.siono == null)
                                                {
                                                    <input type="checkbox" name="siono" value="boxno" id="id-box-no"
                                                           class="custom-control-input"
                                                           onclick="onBoxNo()" checked>
                                                }
                                                else
                                                {
                                                    <input type="checkbox" name="siono" value="boxno" id="id-box-no"
                                                           class="custom-control-input"
                                                           onclick="onBoxNo()">
                                                }
                                                <label class="custom-control-label" for="id-box-no">
                                                    No
                                                </label>
                                            </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-divider"></div>
                            <!-- View -->
                            <div class="boxyes box">

