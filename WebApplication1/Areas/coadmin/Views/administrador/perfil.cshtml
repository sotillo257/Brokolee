                                <div class="form-group row">
                                    <label for="example-since-date-input" class="col-sm-3 col-form-label">Desde</label>
                                    <div class="col-sm-9">
                                        @if (Model.curUser.since == null)
                                        {
                                            <input class="form-control" type="date"
                                                   data-toggle="tooltip" title="Mes / día / año"
                                                   value=""
                                                   name="since" id="example-since-date-input">
                                        }
                                        else
                                        {
                                            <input class="form-control" type="date"
                                                   data-toggle="tooltip" title="Mes / día / año"
                                                   value="@Convert.ToDateTime(Model.curUser.since).ToString("yyyy-MM-dd")"
                                                   name="since" id="example-since-date-input">
                                        }

                                        <small class="form-text text-muted">Mes / día / año</small>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="example-until-date-input" class="col-sm-3 col-form-label">Hasta</label>
                                    <div class="col-sm-9">
                                        @if (Model.curUser.until == null)
                                        {
                                            <input class="form-control" type="date"
                                                   data-toggle="tooltip" title="Mes / día / año"
                                                   value=""
                                                   id="example-until-date-input" name="until">
                                        }
                                        else
                                        {
                                            <input class="form-control" type="date"
                                                   data-toggle="tooltip" title="Mes / día / año"
                                                   value="@Convert.ToDateTime(Model.curUser.until).ToString("yyyy-MM-dd")"
                                                   id="example-until-date-input" name="until">
                                        }

                                        <small class="form-text text-muted">Mes / día / año</small>
                                    </div>
                                </div>
