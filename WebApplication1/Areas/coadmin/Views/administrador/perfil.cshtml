
@{
    ViewBag.Title = "perfil";
    Page.Title = "Brokolee Software - Perfil Administrador";
}

<div class="page-content-wrapper">

    <div class="container-fluid">

        <div class="row">
            <div class="col-sm-12">
                <div class="page-title-box">
                    <div class="float-left">
                        <div class="breadcrumb float-md-right">
                            <h4 class="page-title">Perfil administrador</h4>
                        </div>
                    </div>
                    <div class="float-right">
                        <ul class="breadcrumb float-md-right">
                            <li class="breadcrumb-item"><a href="@Url.Action("panel", "control", new { area = "coadmin" })" class="text-success"><i class="mdi mdi-home-modern"></i> Panel de Control</a></li>
                            <li class="breadcrumb-item active">Perfil administrador</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!-- end page title end breadcrumb -->
        <!-- back button -->
        <div class="row pb-4">
            <div class="col-md-6">
                <a href="javascript:history.back()"><button type="button" class="btn btn-light waves-effect waves-light"><i class="mdi mdi-keyboard-return"></i> Regresar</button></a>
            </div>
        </div>
        <!-- end back button -->

        <div class="row">
            <div class="col-12">
                <div class="card">
                    @using (Html.BeginForm("editprofile", "administrador",
                       FormMethod.Post, new
                       {
                           id = "editprofileForm",
                           @class = "",
                           area = "coadmin",
                           enctype = "multipart/form-data"
                       }))
                    {
                        @Html.AntiForgeryToken()
                        <div class="card-body">
                            <h5 class="d-inline-block">Información del Administrador</h5>
                            <div class="form-divider"></div>
                            <div class="form-group row">
                                <div class="col-sm-3">
                                    @if (Model.curUser.user_img != null)
                                    {
                                        <img src="~/Upload/User_Logo/@Model.curUser.user_img" alt="user-img" class="img-fluid" />
                                    }
                                    else
                                    {
                                        <img src="~/assets/images/user.svg" alt="user-img" class="img-fluid" />
                                    }
                                </div>
                                <div class="col-sm-9">
                                    <div class="input-group mt-2">
                                        <div class="custom-file">
                                            <input type="file" class="custom-file-input" id="inputGroupFile06" name="user_logo"
                                                   data-toggle="tooltip" title="Subir logo en formato jpg o png"/>
                                            <label class="custom-file-label" for="inputGroupFile06">Seleccionar archivo</label>
                                        </div>
                                        <div class="col-sm-3">
                                            <small class="form-text text-muted">Subir logo en formato jpg o png</small>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="form-divider"></div>
                            <div class="form-group row">
                                <label for="example-email-input" class="col-sm-3 col-form-label">Correo electrónico</label>
                                <div class="col-sm-9">
                                    <input class="form-control" type="email" value="@Model.curUser.email" 
                                           name="email" data-toggle="tooltip" title="Base utilizada para identificar las unidades suscritas de varias comunidades adscritas."
                                           placeholder="Correo electrónico del titular" id="example-email-input">
                                    <small class="form-text text-muted">Base utilizada para identificar las unidades suscritas de varias comunidades adscritas.</small>
                                </div>
                            </div>
                            <div class="form-divider"></div>
                            <div class="form-group row">
                                <label for="example-password-input" class="col-sm-3 col-form-label">Contraseña</label>
                                <div class="col-sm-9">
                                    <input class="form-control" type="password" value="@Model.password" name="password"
                                           data-toggle="tooltip" title="Contraseña utilizada para ingresar al sistema"
                                           placeholder="Contraseña" id="example-password-input">
                                    <small class="form-text text-muted">Contraseña utilizada para ingresar al sistema</small>
                                </div>
                            </div>
                            <div class="form-divider"></div>
                            <div class="form-group row">
                                <label for="example-date-input" class="col-sm-3 col-form-label">Fecha de adquisición</label>
                                <div class="col-sm-9">
                                    <input class="form-control" type="date" value="@Convert.ToDateTime(Model.curUser.acq_date).ToString("yyyy-MM-dd")" 
                                           name="acq_date" data-toggle="tooltip" title="Mes / día / año"
                                           id="example-date-input">
                                    <small class="form-text text-muted">Mes / día / año</small>
                                </div>
                            </div>
                            <div class="form-divider"></div>
                            <h6 class="mt-0 header-title">Nombre completo del Titular I</h6>
                            <p>Nombre del titular según aparece en la escritura de compraventa.</p>
                            <div class="form-group row">
                                <label for="example-name1-input" class="col-sm-3 col-form-label">Nombre</label>
                                <div class="col-sm-9">
                                    <input class="form-control" type="text" value="@Model.curUser.first_name1" 
                                           data-toggle="tooltip" title="Nombre del Titular I"
                                           placeholder="Nombre" id="example-name1-input" name="first_name1">
                                    <small class="form-text text-muted">Nombre del Titular I</small>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-lastname1-input" class="col-sm-3 col-form-label">Apellido paterno</label>
                                <div class="col-sm-9">
                                    <input class="form-control" type="text" value="@Model.curUser.last_name1"
                                           data-toggle="tooltip" title="Apellido paterno del Titular I"
                                           placeholder="Apellido paterno" id="example-lastname1-input" name="last_name1">
                                    <small class="form-text text-muted">Apellido paterno del Titular I</small>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-lastname1m-input" class="col-sm-3 col-form-label">Apellido materno</label>
                                <div class="col-sm-9">
                                    <input class="form-control" type="text" value="@Model.curUser.mother_last_name1" 
                                           data-toggle="tooltip" title="Apellido materno del Titular I"
                                           placeholder="Apellido materno" id="example-lastname1m-input" name="mother_last_name1">
                                    <small class="form-text text-muted">Apellido materno del Titular I</small>
                                </div>
                            </div>
                            <div class="form-divider"></div>
                            <h6 class="mt-0 header-title">Nombre completo del Titular II</h6>
                            <p>Nombre del segundo titular según aparece en la escritura de compraventa.</p>
