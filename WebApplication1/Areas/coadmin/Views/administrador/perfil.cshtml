                                <label for="example-name2-input" class="col-sm-3 col-form-label">Nombre</label>
                                <div class="col-sm-9">
                                    <input class="form-control" type="text" value="@Model.curUser.first_name2"
                                           data-toggle="tooltip" title="Nombre del Titular II"
                                           placeholder="Nombre" id="example-name2-input" name="first_name2">
                                    <small class="form-text text-muted">Nombre del Titular II</small>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-lastname2-input" class="col-sm-3 col-form-label">Apellido paterno</label>
                                <div class="col-sm-9">
                                    <input class="form-control" type="text" value="@Model.curUser.last_name2" 
