                                    <label for="example-tenant-name1-input" class="col-sm-3 col-form-label">Nombre</label>
                                    <div class="col-sm-9">
                                        <input class="form-control" type="text" value="@Model.curUser.tenant_first_name1" 
                                               name="tenant_first_name1" data-toggle="tooltip" title="Nombre del Arrendatario I"
                                               placeholder="Nombre" id="example-tenant-name1-input">
                                        <small class="form-text text-muted">Nombre del Arrendatario I</small>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="example-tenant-lastname1-input" class="col-sm-3 col-form-label">Apellido paterno</label>
                                    <div class="col-sm-9">
                                        <input class="form-control" type="text" data-toggle="tooltip" 
                                               title="Apellido paterno del Arrendatario I"
                                               value="@Model.curUser.tenant_last_name1" name="tenant_last_name1"
                                               placeholder="Apellido paterno" id="example-tenant-lastname1-input">
                                        <small class="form-text text-muted">Apellido paterno del Arrendatario I</small>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="example-tenant-lastname1m-input" class="col-sm-3 col-form-label">Apellido materno</label>
                                    <div class="col-sm-9">
                                        <input class="form-control" type="text" name="tenant_mother_last_name1"
                                               value="@Model.curUser.tenant_mother_last_name1" data-toggle="tooltip" 
                                               title="Apellido materno del Arrendatario I"
                                               placeholder="Apellido materno" id="example-tenant-lastname1m-input">
                                        <small class="form-text text-muted">Apellido materno del Arrendatario I</small>
                                    </div>
                                </div>
                                <div class="form-divider"></div>
