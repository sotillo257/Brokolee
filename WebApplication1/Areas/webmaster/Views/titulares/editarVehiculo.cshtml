
@{
    ViewBag.Title = "editar";
    Page.Title = "Editar Titular - Brokolee Software";
}

<script src="~/Scripts/jquery-3.1.1.min.js"></script>




<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">

<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>



<div class="page-content-wrapper">

    <div class="container-fluid">

        <div class="row">
            <div class="col-sm-12">
                <div class="page-title-box">
                    <div class="float-left">
                        <div class="breadcrumb float-md-right">
                            <h4 class="page-title">Editar Titular</h4>
                        </div>
                    </div>
                    <div class="float-right">
                        <ul class="breadcrumb float-md-right">
                            <li class="breadcrumb-item"><a href="@Url.Action("panel", "control", new { area = "webmaster" })" class="text-success"><i class="mdi mdi-home-modern"></i> Panel de Control</a></li>
                            <li class="breadcrumb-item active">Editar Titular</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!-- end page title end breadcrumb -->
        <!-- back button -->
        <div class="row pb-4">
            <div class="col-md-6">
                <a href="javascript:history.back()"><button type="button" class="btn btn-light waves-effect waves-light"><i class="mdi mdi-keyboard-return"></i> Regresar</button></a>
            </div>
        </div>
        <!-- end back button -->

        <div class="row">

            <div class="col-12">
                <div class="card">
                    @using (Html.BeginForm("EditarTitular",
                        "titulares", FormMethod.Post,
                        new
                        {
                            id = "editresidentForm",
                            @class = "",
                            enctype = "multipart/form-data"
                        }))
                    {
                        @Html.AntiForgeryToken()
                        <input type="hidden" value="@Model.editUser.id" name="editID" />
                        <div class="card-body">
                          
                                    <h5 class="d-inline-block">Vehículos autorizados</h5>                                

                          
                            <div class="form-divider"></div>
                            <div class="form-group row">
                                <label for="example-brand-input" class="col-sm-3 col-form-label">Marca</label>
                                <div class="col-sm-9">
                                    <input class="form-control"
                                           type="text" data-toggle="tooltip"
                                           value="@Model.editUser.brand"
                                           name="brand"
                                           placeholder="Marca"
                                           id="example-brand-input">
                                    @* <small class="form-text text-muted">Marca del vehículo</small>*@
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-model-input" class="col-sm-3 col-form-label">Modelo</label>
                                <div class="col-sm-9">
                                    <input class="form-control"
                                           type="text"
                                           value="@Model.editUser.model"
                                           name="model" data-toggle="tooltip"
                                           placeholder="Modelo"
                                           id="example-model-input">
                                    @* <small class="form-text text-muted">Modelo del vehículo</small>*@
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-color-input" class="col-sm-3 col-form-label">Color</label>
                                <div class="col-sm-9">
                                    <input class="form-control"
                                           type="text"
                                           value="@Model.editUser.colour"
                                           name="colour" data-toggle="tooltip"
                                           placeholder="Color"
                                           id="example-color-input">
                                    @* <small class="form-text text-muted">Color del vehículo</small>*@
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-year-input" class="col-sm-3 col-form-label">Año</label>
                                <div class="col-sm-9">
                                    <input class="form-control"
                                           type="number" data-toggle="tooltip"
                                           value="@Model.editUser.year"
                                           placeholder="Año"
                                           id="example-year-input">
                                    @*  <small class="form-text text-muted">Año del vehículo</small>*@
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-cartable-input" class="col-sm-3 col-form-label">Tablilla</label>
                                <div class="col-sm-9">
                                    <input class="form-control"
                                           type="text" data-toggle="tooltip"
                                           value="@Model.editUser.clapboard"
                                           name="clapboard"
                                           placeholder="Tablilla"
                                           id="example-cartable-input">
                                    @* <small class="form-text text-muted">Tablilla del vehículo</small>*@
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-numbercar-input" class="col-sm-3 col-form-label">Número de sello</label>
                                <div class="col-sm-9">
                                    <input class="form-control"
                                           type="number"
                                           value="@Model.editUser.stamp_number"
                                           name="stamp_number" data-toggle="tooltip"
                                           placeholder="Número de sello"
                                           id="example-numbercar-input">
                                    @* <small class="form-text text-muted">Número de sello del vehículo</small>*@
                                </div>
                            </div>
                            <a href="#"><button type="submit" class="btn btn-primary waves-effect waves-light">Guardar</button></a>
                        </div>
                    }
                </div>
            </div>
        </div><!-- end row -->
        <!-- back button -->
        <div class="row pb-4">
            <div class="col-md-6">
                <a href="javascript:history.back()"><button type="button" class="btn btn-light waves-effect waves-light"><i class="mdi mdi-keyboard-return"></i> Regresar</button></a>
            </div>
        </div>
        <!-- end back button -->

    </div><!-- container -->

</div> <!-- Page content Wrapper -->


<script type="text/javascript">

    (function () {

        function filePreview(input) {
            if (input.files && input.files[0]) {

                var reader = new FileReader();

                reader.onload = function (e) {


                    $('#img-fluid').html("<img class='garritas' src='" + e.target.result + "'/>");

                }

                reader.readAsDataURL(input.files[0]);

            }

        }
        $('#inputGroupFile04').change(function () {

            filePreview(this);
        });

    })();



</script>


